<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
	minWidth="-Infinity" prefHeight="205.0" prefWidth="349.0"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="cn.neu.client.PriceController">
	<children>
		<Text layoutX="60.0" layoutY="43.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="商品名称 :" />
		<TextField fx:id="goodsName" layoutX="128.0" layoutY="27.0"
			prefHeight="23.0" prefWidth="161.0" style="-fx-background-radius: 0;" />
		<Text layoutX="60.0" layoutY="84.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="商品价格 :" />
		<TextField fx:id="goodsPrice" disable="false" layoutX="128.0"
			layoutY="68.0" prefHeight="23.0" prefWidth="161.0"
			style="-fx-background-radius: 0;" />
		<ChoiceBox fx:id="goodsType" layoutX="128.0" layoutY="108.0"
			prefHeight="23.0" prefWidth="161.0"
			style="-fx-border-radius: 0; -fx-background-radius: 0;" />
		<Button fx:id="change" layoutX="81.0" layoutY="156.0"
			mnemonicParsing="false" onMouseClicked="#changeOnMouseClicked"
			prefHeight="23.0" prefWidth="54.0" style="-fx-background-radius: 0;"
			text="修改" />
		<Button fx:id="cancel" layoutX="219.0" layoutY="156.0"
			mnemonicParsing="false" onMouseClicked="#cancelOnMouseClicked"
			prefHeight="23.0" prefWidth="54.0" style="-fx-background-radius: 0;"
			text="取消" />
		<Text layoutX="60.0" layoutY="124.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="商品类型 :" />



	</children>
</Pane>
