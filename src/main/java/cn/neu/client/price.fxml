<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane fx:controller="cn.neu.client.PriceController" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="205.0" prefWidth="349.0" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Text layoutX="60.0" layoutY="52.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="商品名称 :" />
		<TextField fx:id="goodsName" disable="true" layoutX="128.0"
			layoutY="36.0" prefHeight="23.0" prefWidth="161.0"
			style="-fx-background-radius: 0;" />
		<Text layoutX="60.0" layoutY="96.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="商品价格 :" />
		<TextField fx:id="goodsPrice" disable="false" layoutX="128.0"
			layoutY="80.0" prefHeight="23.0" prefWidth="161.0"
			style="-fx-background-radius: 0;" />
		<Button fx:id="change" layoutX="80.0" layoutY="140.0"
			mnemonicParsing="false" onMouseClicked="#changeOnMouseClicked"
			prefHeight="23.0" prefWidth="54.0" style="-fx-background-radius: 0;"
			text="修改" />
		<Button fx:id="cancel" layoutX="222.0" layoutY="140.0"
			mnemonicParsing="false" onMouseClicked="#cancelOnMouseClicked"
			prefHeight="23.0" prefWidth="54.0" style="-fx-background-radius: 0;"
			text="取消" />


	</children>
</Pane>
